@inject IJSRuntime JS


<MudCarousel Class="mud-width-full" Style="height:400px" ShowArrows="false" ShowBullets="false" EnableSwipeGesture="@enableSwipeGesture" AutoCycle="true" TData="object">
    <MudCarouselItem  Style="justify-content:space-between" Color="@Color.Primary">

        <video autoPlay loop muted style="width:100%">
            <source src="/videos/code_flythough_loop_01_videvo_preview.mp4" type="video/mp4" />
        </video>
    </MudCarouselItem> bn 
    <MudCarouselItem Transition="fade" Color="@Color.Secondary">
        <video autoPlay loop muted style="width:100%">
            <source src="/videos/spinningworld.mp4" type="video/mp4" />
        </video>
    </MudCarouselItem>
    <MudCarouselItem Transition="transition">
        <div class="d-flex" style="height:100%">
            <MudIcon Class="mx-auto my-auto" Icon="@Icons.Custom.Brands.MudBlazor" Color="@Color.Primary" Size="@Size.Large" />
        </div>
    </MudCarouselItem>
</MudCarousel>



@code{
    private bool arrows = true;
    private bool bullets = true;
    private bool enableSwipeGesture = true;
    private bool autocycle = true;
    private Transition transition = Transition.Slide;
    private Transition fade = Transition.Fade;



}